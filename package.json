  "homepage": "https://fribbels.github.io/hsr-optimizer",
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "ag-grid-community": "^31.0.3",
    "ag-grid-react": "^31.0.1",
    "antd": "^5.15.1",
    "btoa": "^1.2.1",
    "fast-sort": "^3.4.0",
    "html-to-image": "^1.11.11",
    "object-hash": "^3.0.0",
    "path": "^0.12.7",
    "plotly.js": "^2.29.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-countup": "^6.5.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-plotly.js": "^2.6.0",
    "semver": "^7.6.0",
    "string-similarity": "^4.0.4",
    "styled-components": "^6.0.8",
    "tinygradient": "^1.1.5",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4",
    "workerpool": "^6.5.1",
    "zustand": "^4.4.7"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx",
    "vitest": "vitest --dir src/lib --no-watch",
    "vitest:watch": "vitest --dir src/lib --watch",
    "test": "npx playwright test --trace on",
    "test:generate": "npx playwright codegen localhost:3000/hsr-optimizer",
    "test:ui": "npm run test -- --ui",
    "test:report": "npx playwright show-report",
    "typecheck": "tsc --noEmit -p ./tsconfig.json",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@stylistic/eslint-plugin": "^1.5.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^20.11.6",
    "@types/object-hash": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-legacy": "^5.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.1.0",
    "jsdom": "^24.0.0",
    "terser": "^5.27.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1"
  }
}
